#include <cstdio>
#include <iostream>
#include <cstring>
using namespace std;

struct N{
    int a,b,c;
    bool operator < (const N n1,const N n2){
        double tmp1 = n1.b * 1.0; tmp1 /= n1.c;
        double tmp2 = n2.b * 1.0; tmp2 /= n2.c;
        if(tmp1 > tmp2) return 1;
        return 0;
    }
}
const int MX = 3007
const int INF = 0x3f3f3f3f
int num[MX][3];
int dp[MX][2];
void solve(){
    int n;cin>> n;
    int ans = 0;
    int j = 0;
    for(int i = 0; i < n; i++){
        scanf("%d%d%d",&num[j][0],&num[j][1],&num[j][2])
        if(num[j][2] == 0){
            ans += num[j][0];
            continue;
        }
        j++;
    }
    int len = j;
}
int main(){
    int _;
    while(cin>>_){
        while(_--){
            solve();
        }
    }
    return 0;
}
